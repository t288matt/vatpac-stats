          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  airport_code,\n  COUNT(CASE WHEN movement_type = 'departure' THEN 1 END) as departures,\n  COUNT(CASE WHEN movement_type = 'arrival' THEN 1 END) as arrivals,\n  COUNT(*) as total_movements\nFROM (\n  SELECT departure as airport_code, 'departure' as movement_type\n  FROM flights \n  WHERE status = 'active' AND departure LIKE 'Y%'\n  UNION ALL\n  SELECT arrival as airport_code, 'arrival' as movement_type\n  FROM flights \n  WHERE status = 'active' AND arrival LIKE 'Y%'\n) movements\nGROUP BY airport_code\nORDER BY total_movements DESC\nLIMIT 10",
          "refId": "A",
