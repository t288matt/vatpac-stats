          "rawQuery": true,
          "rawSql": "SELECT\n  aircraft_type,\n  COUNT(*) as flights\nFROM flights\nWHERE (departure IN ('YBBN', 'YSSY', 'YMML', 'YPPH', 'YSCL', 'YBCS', 'YPDN', 'YBRM', 'YBAF', 'YMAV', 'YBRK', 'YBCG', 'YBTL', 'YBRN', 'YBAS', 'YPDL', 'YPPN') \n    OR arrival IN ('YBBN', 'YSSY', 'YMML', 'YPPH', 'YSCL', 'YBCS', 'YPDN', 'YBRM', 'YBAF', 'YMAV', 'YBRK', 'YBCG', 'YBTL', 'YBRN', 'YBAS', 'YPDL', 'YPPN'))\n  AND aircraft_type IS NOT NULL\n  AND aircraft_type != ''\n  AND created_at >= NOW() - INTERVAL '7 days'\nGROUP BY aircraft_type\nORDER BY flights DESC\nLIMIT 10",
