          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT\n  date_trunc('${time_granularity}', created_at) as time,\n  COUNT(*) FILTER (WHERE flight_rules = 'I') as \"IFR Flights\",\n  COUNT(*) FILTER (WHERE flight_rules = 'V') as \"VFR Flights\",\n  COUNT(*) FILTER (WHERE flight_rules IS NULL OR flight_rules = '' OR flight_rules NOT IN ('I', 'V')) as \"Unknown Rules\",\n  ROUND(COUNT(*) FILTER (WHERE flight_rules = 'I') * 100.0 / NULLIF(COUNT(*), 0), 1) as \"IFR Percentage\"\nFROM (\n  SELECT DISTINCT cid, deptime, callsign, departure, arrival, flight_rules, created_at\n  FROM flights\n  WHERE created_at >= NOW() - INTERVAL '${time_range}'\n) unique_flights\nGROUP BY time\nORDER BY time;",
          "refId": "A",
