          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  CASE \n    WHEN flight_rules = 'I' THEN 'IFR'\n    WHEN flight_rules = 'V' THEN 'VFR'\n    ELSE 'Unknown'\n  END as flight_type,\n  COUNT(*) as count,\n  ROUND(COUNT(*) * 100.0 / SUM(COUNT(*)) OVER(), 1) as percentage\nFROM (\n  SELECT DISTINCT cid, deptime, callsign, departure, arrival, flight_rules\n  FROM flights\n  WHERE created_at >= NOW() - INTERVAL '${time_range}'\n) unique_flights\nGROUP BY flight_type\nORDER BY count DESC;",
          "refId": "A",
