          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH latest_flights AS (\n  SELECT DISTINCT ON (callsign) \n    callsign,\n    last_updated\n  FROM flights\n  WHERE status = 'active'\n  ORDER BY callsign, last_updated DESC\n),\nstats AS (\n  SELECT \n    COUNT(*) as active_flights,\n    MAX(last_updated) as latest_update,\n    EXTRACT(EPOCH FROM (NOW() - MAX(last_updated))) as seconds_since_update\n  FROM latest_flights\n)\nSELECT \n  active_flights as \"Active Flights\",\n  latest_update as \"Latest Update\",\n  ROUND(seconds_since_update::numeric, 1) as \"Seconds Since Update\",\n  CASE \n    WHEN seconds_since_update < 60 THEN 'Fresh' \n    WHEN seconds_since_update < 300 THEN 'Stale' \n    ELSE 'Very Stale' \n  END as \"Data Status\"\nFROM stats",
          "refId": "A",
