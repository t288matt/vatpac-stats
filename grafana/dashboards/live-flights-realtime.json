          },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT\n  (date_trunc('minute', last_updated AT TIME ZONE 'UTC'))::timestamp as time,\n  CASE WHEN COUNT(DISTINCT callsign) > 0 THEN \n    ROUND(COUNT(DISTINCT CASE WHEN flight_rules = 'I' THEN callsign END) * 100.0 / COUNT(DISTINCT callsign), 1)\n    ELSE 0 END as \"IFR %\",\n  CASE WHEN COUNT(DISTINCT callsign) > 0 THEN \n    ROUND(COUNT(DISTINCT CASE WHEN flight_rules = 'V' THEN callsign END) * 100.0 / COUNT(DISTINCT callsign), 1)\n    ELSE 0 END as \"VFR %\",\n  CASE WHEN COUNT(DISTINCT callsign) > 0 THEN \n    ROUND(COUNT(DISTINCT CASE WHEN flight_rules IS NULL OR flight_rules = '' THEN callsign END) * 100.0 / COUNT(DISTINCT callsign), 1)\n    ELSE 0 END as \"Unknown %\"\nFROM flights\nWHERE last_updated >= NOW() - INTERVAL '2 hours'\nGROUP BY time\nORDER BY time;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
